{
  "name": "toast-digital-garden-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@sector/breadbox": "^0.0.4"
  },
  "scripts": {
    "build": "yarn build:css && toast incremental",
    "build:css": "NODE_ENV=production tailwindcss build src/style/main.css --output public/style.css",
    "prepare": "breadbox --dest public/web_modules",
    "breadbox": "breadbox --dest public/web_modules"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/preact": "^1.6.6",
    "@sector/babel-plugin-package-import": "^0.3.1",
    "@sindresorhus/slugify": "^1.0.0",
    "cloudinary": "^1.22.0",
    "dotenv": "^8.2.0",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "preact": "^10.4.4",
    "react-helmet": "^6.1.0",
    "rehype-local-image-to-cloudinary": "^1.0.1",
    "rehype-slug": "^3.0.0",
    "tailwindcss": "^1.4.6",
    "toast": "^0.2.6"
  },
  "snowpack": {
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "@mdx-js/preact",
      "preact/compat",
      "react-helmet"
    ]
  }
}
